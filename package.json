{
  "name": "fastify-sample",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts --ignore-watch node_modules",
    "build": "tsc -p \"./\"",
    "test": "yarn build && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "commit": "git cz",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js)\" --write"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.23.1",
    "@types/mongoose": "^5.11.97",
    "awilix": "^6.0.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.25.0",
    "fastify-awilix": "^1.2.0",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.13.0",
    "json-schema-to-ts": "^1.6.4",
    "mongodb": "^4.2.2",
    "mongoose": "^6.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "tap": "^15.1.5",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.3"
  }
}
