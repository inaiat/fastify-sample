{
  "name": "fastify-sample",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist",
    "lint": "eslint . --fix",
    "check-push": "yarn lint && yarn jscpd && yarn tsc",
    "dev": "development=true nodemon | pino-pretty -c -i pid,hostname -S -t 'sys:standard'",
    "prepare": "husky install",
    "test": "ava",
    "coverage": "c8 ava"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.1.0",
    "@fastify/awilix": "^3.0.0",
    "@fastify/sensible": "^5.1.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/type-provider-typebox": "^2.1.0",
    "@sinclair/typebox": "^0.24.19",
    "@sindresorhus/is": "^5.2.0",
    "awilix": "7.0.3",
    "env-schema": "5.0.0",
    "fastify": "4.2.1",
    "fastify-plugin": "4.0.0",
    "mongodb": "4.8.0",
    "nanoid": "4.0.0",
    "neverthrow": "4.4.2",
    "papr": "7.0.1"
  },
  "devDependencies": {
    "@jscpd/badge-reporter": "^3.4.5",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.212",
    "@swc/helpers": "^0.4.3",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "ava": "^4.3.1",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jscpd": "^3.4.5",
    "nodemon": "^2.0.19",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "strong-mock": "^7.3.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "jscpd": {
    "pattern": "src/**/*.ts",
    "reporters": [
      "html",
      "console",
      "badge"
    ],
    "ignore": [
      "**/__snapshots__/**"
    ],
    "absolute": true,
    "gitignore": true
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "tsconfig.json",
      ".env"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node --esm ./src",
    "env": {
      "NODE_DEBUG": 9229,
      "NODE_ENV": "development"
    }
  },
  "keywords": [
    "functional",
    "fastify",
    "neverthrow",
    "awilix",
    "dependency-injection",
    "typesafe",
    "typescript",
    "esm",
    "mongodb",
    "ava",
    "test"
  ],
}