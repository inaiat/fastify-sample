{
  "name": "fastify-sample",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist",
    "lint": "eslint . --fix",
    "check-push": "yarn lint && jscpd && yarn coverage && yarn make-badges && yarn tsc",
    "dev": "development=true nodemon | pino-pretty -c -i pid,hostname -S -t 'sys:standard'",
    "prepare": "husky install",
    "test": "ava",
    "coverage": "c8 ava",
    "make-badges": "istanbul-badges-readme"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.2.0",
    "@fastify/awilix": "^3.0.0",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/sensible": "^5.1.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/type-provider-typebox": "^2.2.0",
    "@inaiat/fastify-papr": "^0.0.5",
    "@sinclair/typebox": "^0.24.27",
    "@sindresorhus/is": "^5.3.0",
    "awilix": "^7.0.3",
    "env-schema": "^5.0.0",
    "fastify": "^4.3.0",
    "fastify-plugin": "^4.1.0",
    "mongodb": "^4.8.1",
    "nanoid": "^4.0.0",
    "neverthrow": "^5.0.0"
  },
  "devDependencies": {
    "@jscpd/badge-reporter": "^3.4.5",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.212",
    "@swc/helpers": "^0.4.3",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.2.2",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "istanbul-badges-readme": "^1.8.2",
    "jscpd": "^3.4.5",
    "nodemon": "^2.0.19",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "strong-mock": "^7.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "jscpd": {
    "pattern": "src/**/*.ts",
    "reporters": [
      "html",
      "console",
      "badge"
    ],
    "ignore": [
      "**/__snapshots__/**"
    ],
    "absolute": true,
    "gitignore": true,
    "reportersOptions": {
      "badge": {
        "path": "./assets/jscpd-badge.svg"
      }
    }
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "c8": {
    "reporter": [
      "json-summary"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "tsconfig.json",
      ".env"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node --experimental-specifier-resolution=node --esm ./src",
    "env": {
      "NODE_DEBUG": 9229,
      "NODE_ENV": "development"
    }
  },
  "keywords": [
    "functional",
    "fastify",
    "neverthrow",
    "awilix",
    "dependency-injection",
    "typesafe",
    "typescript",
    "esm",
    "mongodb",
    "ava",
    "test"
  ]
}
